{
  "name": "@stencil/starter",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil Starter App",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "gulp && stencil build --prerender",
    "dev": "sd concurrent \"gulp && stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "heroku": "npm i && gulp && stencil build --dev && npm run node-server",
    "node-server": "node express-server.js"
  },
  "dependencies": {
    "@stencil/core": "next",
    "@stencil/router": "0.0.9",
    "@types/bootstrap": "^3.3.36",
    "bootstrap": "^4.0.0-beta.3",
    "express": "^4.16.2",
    "gulp": "^3.9.1",
    "gulp-markdown": "^2.0.1",
    "highlight.js": "^9.12.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-starter.git"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ionic-team/stencil"
  },
  "homepage": "https://github.com/ionic-team/stencil"
}
